<h3>Update an artwork</h3>

<div class="container">
  <%= simple_form_for [ @artwork ] do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :dimension %>
    <%= f.input :category %>
    <%= f.submit "Add artwork", class: "btn btn-primary" %>
  <% end %>

  <%= link_to 'Back', artworks_path %>
</div>